<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../favicon.ico">

    <title>{{ pageName }}</title>

    <link href="/jquery-ui-1.12.1.custom/jquery-ui.css" rel="stylesheet">
    <link href="/vendor/twbs/bootstrap/dist/css/bootstrap.css" rel="stylesheet">
    <link href="/css/result.css" rel="stylesheet">

    <script>
        var destinations = [
            {% for destination in destinations %}
                {label: '{{ destination[0] }}', country: '{{ destination[0] }}', region: '', place: '', category: 'country'},
                {label: '{{ destination[0] }} &gt; {{ destination[1] }}', country: '{{ destination[0] }}', region: '{{ destination[1] }}', place: '', category: 'region'},
                {label: '{{ destination[0] }} &gt; {{ destination[1] }} &gt; {{ destination[2] }}', country: '{{ destination[0] }}', region: '{{ destination[1] }}', place: '{{ destination[2] }}', category: 'place'},
            {% endfor %}
        ];
    </script>

</head>